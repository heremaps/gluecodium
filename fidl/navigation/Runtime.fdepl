import "classpath:/navigation/LegacySpec.fdepl"
import "./Runtime.fidl"
import "./Module.fidl"

define navigation.LegacySpec for interface navigation.Runtime
{
  method createRuntime {
    CallSemantics = synchronous
    Static = true
    Creates = &navigation.Runtime
  }
  method createModule {
    Creates = &navigation.Module
  }
}