import 'dart:ffi';
import 'package:ffi/ffi.dart';

import '_library_init.dart' as __lib;

{{!! TODO: APIGEN-1773: wrap this in String<->Pointer<Void> conversion functions }}
final std_string_create_handle = __lib.nativeLibrary.lookupFunction<
    Pointer<Void> Function(Pointer<Utf8>),
    Pointer<Void> Function(Pointer<Utf8>)
  >('std_string_create_handle');
final std_string_release_handle = __lib.nativeLibrary.lookupFunction<
    Void Function(Pointer<Void>),
    void Function(Pointer<Void>)
  >('std_string_release_handle');
final std_string_get_value = __lib.nativeLibrary.lookupFunction<
    Pointer<Utf8> Function(Pointer<Void>),
    Pointer<Utf8> Function(Pointer<Void>)
  >('std_string_get_value');
