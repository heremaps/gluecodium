#pragma once
#include "Hash.h"
#include <chrono>

{{#internalNamespace}}
namespace {{.}} {
{{/internalNamespace}}

template<>
struct hash<std::chrono::system_clock::time_point>
{
    size_t operator( )( const std::chrono::system_clock::time_point& t ) const noexcept
    {
        return hash<std::chrono::system_clock::time_point::duration::rep>{}( t.time_since_epoch( ).count( ) );
    }
};
{{#internalNamespace}}
}
{{/internalNamespace}}
