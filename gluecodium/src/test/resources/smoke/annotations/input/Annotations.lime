package smoke

@Internal
class Annotations {
    fun testOptional(self: Annotations): Boolean?
}

class OuterPublicClazz {
    @Internal
    class InnerInternalClazz {
        fun someFunction(): Boolean
    }
}

@Internal
interface OuterInternalInterface {
    fun someFunction(): Int
    property someProperty: Int
    static property someStaticProperty: Int
}

@Internal
enum OuterInternalEnum {
   FIRST = 1,
   SECOND = 2
}

@Internal
exception OuterInternal(OuterInternalEnum)

@Internal
lambda OuterInternalLambda = (String) -> Int

@Internal
struct OuterInternalStruct {
    someField: Int
}
