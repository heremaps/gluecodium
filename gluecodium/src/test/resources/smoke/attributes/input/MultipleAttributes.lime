package smoke

class MultipleAttributes {
    @Java(Attribute = "Foo")
    @Java(Attribute = "Bar")
    fun noLists2()
    @Java(Attribute = "Foo")
    @Java(Attribute = "Bar")
    @Java(Attribute = "Baz")
    fun noLists3()
    @Java(Attribute = ["Foo", "Bar"])
    @Java(Attribute = "Baz")
    fun listFirst()
    @Java(Attribute = "Foo")
    @Java(Attribute = ["Bar", "Baz"])
    fun listSecond()
    @Java(Attribute = ["Foo", "Bar"])
    @Java(Attribute = ["Baz", "Fizz"])
    fun twoLists()
}
