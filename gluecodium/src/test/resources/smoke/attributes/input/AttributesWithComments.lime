package smoke

// Class comment
@Java(Attribute = "OnClass")
class AttributesWithComments {
   // Function comment
   @Java(Attribute = "OnFunctionInClass")
   fun veryFun()
   // Property comment
   // @get Getter comment
   // @set Setter comment
   @Java(Attribute = "OnPropertyInClass")
   property prop: String
   // Const comment
   @Java(Attribute = "OnConstInClass")
   const pi: Boolean = false

   struct SomeStruct {
      // Field comment
      @Java(Attribute = "OnField")
      field: String
   }
}

@Deprecated
@Java(Attribute = "OnClass")
class AttributesWithDeprecated {
   @Deprecated
   @Java(Attribute = "OnFunctionInClass")
   fun veryFun()
   @Deprecated
   @Java(Attribute = "OnPropertyInClass")
   property prop: String
   @Deprecated
   @Java(Attribute = "OnConstInClass")
   const pi: Boolean = false

   struct SomeStruct {
      @Deprecated
      @Java(Attribute = "OnField")
      field: String
   }
}
