package navigation

/**
 * Simple test fidl
 *
 */
interface Simple {
  version {
    major 1
    minor 0
  }

  /**
   * Sing a song
   */
  method sing {
    in {
      /** Title. **/
      String title
    }

    out {
      /** Text of the song. **/
      String text
    }

    /** None on success, or another error code otherwise. **/
    error NavigationTypes.ErrorCode
  }
}
