package examples

interface ProfileManager {

    <** @description: Representing type for instances of the profile manager **>
    public typedef ProfileManager is undefined

    method createProfile {
        in {
            String username
        }
    }

    method changeProfile {
        in {
            String username
        }
        out {
            String oldUsername
        }
    }

    method deleteProfile {
        in {
            String username
        }
    }
}

interface ProfileManagerInterface {

    public typedef ProfileManagerInterface is undefined

    method createProfile {
        in {
            String username
        }
    }
}

interface ProfileManagerFactory {

    method createProfileManager {
        out {
            ProfileManager.ProfileManager profileManager
        }
    }

    method createProfileManagerInterface {
        out {
            ProfileManagerInterface.ProfileManagerInterface profileManagerInterface
        }
    }
}
