package smoke

interface ArrayMethodOverloads {

    map StringToIdMap { String to UInt64 }
    map IdToStringMap { UInt64 to String }

    method doStuff:noArgOverload {
    }

    method doStuff:intOverload {
        in {
            Int32 input
        }
    }

    method doStuff:intArrayOverload {
        in {
            Int32[] input
        }
    }

    method doStuff:stringArrayOverload {
        in {
            String[] input
        }
    }

    method doStuff:stringToIdMapOverload {
        in {
            StringToIdMap input
        }
    }

    method doStuff:idToStringMapOverload {
        in {
            IdToStringMap input
        }
    }

    method doStuff:arrayAndIntOverload {
        in {
            Int32[] arrayParam
            Int32 intParam
        }
    }

    method doStuff:intAndArrayOverload {
        in {
            Int32 intParam
            Int32[] arrayParam
        }
    }

    method doOtherStuff {
        in {
            Int32[] input
        }
    }

    method doMoreStuff {
        in {
            StringToIdMap input
        }
    }
}
