package smoke

interface SimpleInstantiable {

    public typedef SimpleInstantiable is undefined

    method setStringValue {
        in {
            String stringValue
        }
    }

    method getStringValue {
        out {
           String stringValue
        }
    }
}

interface NestedInstantiable {

    public typedef NestedInstantiable is undefined

    method setSameTypeInstances {
        in {
            SimpleInstantiable.SimpleInstantiable instanceOne
            SimpleInstantiable.SimpleInstantiable instanceTwo
        }
    }

    method getInstanceOne {
        out {
            SimpleInstantiable.SimpleInstantiable instanceOne
        }
    }
}

interface InstanceWithStruct {

    version {
        major 0
        minor 1
    }

    public typedef InstanceWithStruct is undefined

    struct InnerStruct {
        Int8 value
    }

    method innerStructMethod {
        in {
            InnerStruct inputStruct
        }
        out {
            InnerStruct outputStruct
        }
    }

    struct StructWithInstance {
        SimpleInstantiable.SimpleInstantiable instance
        <** @description: This is some very useful field. **>
        SimpleInstantiable.SimpleInstantiable instanceWithComment
    }

    method structWithInstanceMethod {
        in {
            StructWithInstance input
        }
        out {
            StructWithInstance output
        }
    }
}
