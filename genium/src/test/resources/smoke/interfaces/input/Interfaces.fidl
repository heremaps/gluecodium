package smoke

interface SimpleInterface {

    public typedef SimpleInterface is undefined

    method setStringValue {
        in {
            String stringValue
        }
    }

    method getStringValue {
        out {
           String stringValue
        }
    }
}

interface NestedInterface {

    public typedef NestedInterface is undefined

    method setSameTypeInstances {
        in {
            SimpleInterface.SimpleInterface interfaceOne
            SimpleInterface.SimpleInterface interfaceTwo
        }
    }

    method getInstanceOne {
        out {
            SimpleInterface.SimpleInterface interfaceOne
        }
    }

    method getInstanceTwo {
        out {
            SimpleInterface.SimpleInterface interfaceTwo
        }
    }

    method makeMoreExternal:withInterface {
        in {
            ExternalInterface.ExternalInterface input
        }
        out {
            VeryExternalInterface.VeryExternalInterface result
         }
    }

    method makeMoreExternal:withStruct {
        in {
            ExternalInterface.some_Struct input
        }
        out {
            VeryExternalInterface.some_Struct result
         }
    }

    method makeMoreExternal:withEnum {
        in {
            ExternalInterface.some_Enum input
        }
        out {
            VeryExternalInterface.some_Enum result
         }
    }
}

interface InterfaceWithStruct {

    public typedef InterfaceWithStruct is undefined

    struct InnerStruct {
        Int8 value
    }

    method innerStructMethod {
        in {
            InnerStruct inputStruct
        }
        out {
            InnerStruct outputStruct
        }
    }
}

interface ExternalInterface {

    public typedef ExternalInterface is undefined

    public struct some_Struct {
        String some_Field
    }

    public enumeration some_Enum {
        some_Value
    }

    attribute String some_Attribute readonly

    method some_Method {
        in { Int8 some_Parameter }
    }
}

interface VeryExternalInterface {

    public typedef VeryExternalInterface is undefined

    public struct some_Struct {
        String some_Field
    }

    public enumeration some_Enum {
        some_Value
    }

    attribute String some_Attribute

    method some_Method {
        in { Int8 some_Parameter }
    }
}
