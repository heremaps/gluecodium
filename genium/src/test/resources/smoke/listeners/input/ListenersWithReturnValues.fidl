package smoke

import smoke.CalculationResult.CalculationResult from "Listeners.fidl"

interface ListenersWithReturnValues {

    public typedef ListenersWithReturnValues is undefined

    map StringToDouble { String to Double }

    struct ResultStruct {
        Double result
    }

    enumeration ResultEnum {
        NONE
        RESULT
    }

    method fetchData:double {
        out {
            Double result
        }
    }

    method fetchData:string {
        out {
            String result
        }
    }

    method fetchData:Struct {
        out {
            ResultStruct result
        }
    }

    method fetchData:enum {
        out {
            ResultEnum result
        }
    }

    method fetchData:Array {
        out {
            Double[] Data
        }
    }

    method fetchData:Map {
        out {
            StringToDouble Data
        }
    }

    method fetchData:instance {
        out {
            CalculationResult Data
        }
    }
}
