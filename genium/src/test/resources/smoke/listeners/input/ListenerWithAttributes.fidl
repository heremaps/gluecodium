package smoke

import smoke.CalculationResult.CalculationResult from "Listeners.fidl"

interface ListenerWithAttributes {

    public typedef ListenerWithAttributes is undefined

    map StringToDouble { String to Double }

    struct ResultStruct {
        Double result
    }

    enumeration ResultEnum {
        NONE
        RESULT
    }

    attribute String message
    attribute CalculationResult packedMessage
    attribute ResultStruct structuredMessage
    attribute ResultEnum enumeratedMessage
    attribute String[] arrayedMessage
    attribute StringToDouble mappedMessage
    attribute ByteBuffer bufferedMessage
}
