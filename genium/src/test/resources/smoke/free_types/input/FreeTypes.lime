package smoke

typealias FreeTypeDef = Date

struct FreePoint {
    x: Double
    y: Double

    fun flip(): FreePoint

    const A_BAR: FreeEnum = FreeEnum.BAR
}

enum FreeEnum {
    FOO,
    BAR
}

exception FreeEnumException(FreeEnum)

class UseFreeTypes {
    fun doStuff(point: FreePoint, mode: FreeEnum): FreeTypeDef throws FreeEnumException
}

enum UnusedTopLevelEnum {
    DOESNT_WORK,
    CRASHED_ANYWAY
}

struct UnusedTopLevelPoint {
    foo: String
}
