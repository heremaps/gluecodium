package test
import test.TypeCollection.* from "./TypeCollection.fidl"

interface PlainDataStructuresFromTypeCollection {
    version {
        major 0
        minor 1
    }

    <** @description: Static method to create a point **>
    method createPoint {
        in {
            Double x
            Double y
        }
        out {
            Point point
        }
    }

    <** @description: Static method to exchange x and y values of a point **>
    method swapPointCoordinates {
        in {
            Point input
        }
        out {
            Point output
        }
    }

    <** @description:
    Static test method which creates a colored line
    **>
    method createLine {
        in {
            Point pointA
            Point pointB
        }
        out {
            Line output
        }
    }

    <** @description:
    Static test method which creates a colored line
    **>
    method createColoredLine {
        in {
            Line line
            Color color
        }
        out {
            ColoredLine output
        }
    }

    <** @description:
    Static test method which modifies the AllTypesStruct
    **>
    method modifyAllTypesStruct {
        in {
            AllTypesStruct input
        }
        out {
            AllTypesStruct output
        }
    }
}
