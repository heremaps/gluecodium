package test

interface UnloadedClass {
    public typedef UnloadedClass is undefined

    method create {
        out {
            UnloadedClass instance
        }
    }

    method increment {
        in {
            UInt8 input
        }
        out {
            UInt8 output
        }
    }
}

interface ThreadedListener {

    public typedef ThreadedListener is undefined

    method onEvent {
        in {
            String message
        }
        out {
            Int64 response
        }
    }

    method unloaded {
        in {
            UnloadedClass.UnloadedClass unloaded
        }
    }
}

interface ThreadedNotifier {
    public typedef ThreadedNotifier is undefined

    method createOnNewThread {
        out {
            ThreadedNotifier instance
        }
    }

    method notify {
        in {
            ThreadedListener.ThreadedListener listener
            String message
        }
        out {
            Int64 response
        }
    }
}
