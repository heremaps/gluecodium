package test

typeCollection Constants {

    const Int32 intConstant = -11
    const UInt32 uintConstant = 0xFFFFFFFF
    const Float floatConstant = 2.71f
    const Double doubleConstant = -3.14d
    const String stringConstant = "Foo bar"
    const StateEnum enumConstant = StateEnum.ON

    enumeration StateEnum {
        OFF
        ON
    }
}

interface ConstantsInterface {

    const Int32 intConstant = -11
    const UInt32 uintConstant = 42
    const Float floatConstant = 2.71f
    const Double doubleConstant = -3.14d
    const String stringConstant = "Foo bar"
    const StateEnum enumConstant = StateEnum.ON

    public enumeration StateEnum {
        OFF
        ON
    }
}

interface UseTypeCollectionConstants {

    method getIntConstant {
        out {
            Int32 result
        }
    }

    method getUintConstant {
        out {
            UInt32 result
        }
    }

    method getFloatConstant {
        out {
            Float result
        }
    }

    method getDoubleConstant {
        out {
            Double result
        }
    }

    method getStringConstant {
        out {
            String result
        }
    }

    method getEnumConstant {
        out {
            Constants.StateEnum result
        }
    }
}

interface UseInterfaceConstants {

