package test

interface MethodOverloads {

    method isBoolean:boolOverload {
        in {
            Boolean input
        }
        out {
            Boolean output
        }
    }

    method isBoolean:intOverload {
        in {
            Int8 input
        }
        out {
            Boolean output
        }
    }

    method isBoolean:stringOverload {
        in {
            String input
        }
        out {
            Boolean output
        }
    }

    struct Point {
        Double x
        Double y
    }

    method isBoolean:structOverload {
        in {
            Point input
        }
        out {
            Boolean output
        }
    }

    method isBoolean:everythingOverload {
        in {
            Boolean input1
            Int8 input2
            String input3
            Point input4
        }
        out {
            Boolean output
        }
    }

    array StringArray of String
    array IntArray of Int8

    method isBoolean:stringArrayOverload {
       in {
           StringArray input
       }
       out {
           Boolean output
       }
    }

    method isBoolean:intArrayOverload {
      in {
          IntArray input
      }
      out {
          Boolean output
      }
    }
}

interface SpecialNames {

    typedef SpecialNames is undefined

    method create {
    }

    method release {
    }

    method createProxy {
    }
}

interface ConstructorOverloads {

    typedef ConstructorOverloads is undefined

    enumeration ErrorEnum {
        NONE
        CRASHED
    }

    method create:noParameters {
    }

    method create:singleStringParameter {
        in {
            String input
        }
        out {
            Boolean dummy
        }
    }

    method create:singleBoolParameter {
        in {
            Boolean input
        }
    }

    method create:twoParameters {
        in {
            String stringInput
            Boolean booleanInput
        }
    }

    method create:withError {
        in {
            Double input
        }
        error ErrorEnum
    }

    method create:arrayParameter {
        in {
            Double[] input
        }
    }
}

interface ChildConstructorOverloads extends ConstructorOverloads {

    typedef ChildConstructorOverloads is undefined

    method create:noParametersChild {
        out {
            String dummy
        }
    }

    method create:withErrorChild {
        in {
            Double input
        }
        out {
            Float dummy
        }
        error ConstructorOverloads.ErrorEnum
    }
}
