import "classpath:/GeniumExtensions.fdepl"
import "./NotNull.fidl"

define GeniumExtensions for interface test.NotNullStatic
{
    struct NotNullStruct {
        notNullField {
            NotNull = true
        }
    }

    method notNullTopDownRoundTrip {
        Static = true
        in {
            input {
                NotNull = true
            }
        }
        out {
            output {
                NotNull = true
            }
        }
    }

    method notNullBottomUpRoundTrip {
        Static = true
    }

    method notNullBottomUpAttributeRoundTrip {
        Static = true
    }
}

define GeniumExtensions for interface test.NotNullListener
{
    IsInterface = true

    method notNullBottomUpRoundTrip {
        in {
            input {
                NotNull = true
            }
        }
        out {
            output {
                NotNull = true
            }
        }
    }

    attribute notNullAttribute {
        NotNull = true
    }
}

define GeniumExtensions for interface test.NotNullPayload
{
    method create {
        Static = true
        out {
            result {
                NotNull = true
            }
        }
    }
}
