package test

typeCollection Forecast {

    struct ForecastData {
        Int32 lowestDegree
        Int32 highestDegree
    }

    map CityDataMap { String to ForecastData }
}

interface ForecastFactory {

    method createProvider {
        out {
          ForecastProvider.ForecastProvider provider
        }
    }

    method createListener {
        out {
          ForecastListener.ForecastListener listener
        }
    }
}

interface ForecastProvider {

    public typedef ForecastProvider is undefined

    method inform {

       in {
           ForecastListener.ForecastListener listener
       }
    }
}

interface ForecastListener {

    public typedef ForecastListener is undefined

    method onForecastDataProvided {
        in {
            Forecast.CityDataMap data
        }
    }
}

