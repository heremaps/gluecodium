package test

interface NotNullStatic {

    public struct NotNullStruct {
        NotNullPayload.NotNullPayload notNullField
    }

    method notNullTopDownRoundTrip {
        in {
            NotNullPayload.NotNullPayload input
        }
        out {
            NotNullPayload.NotNullPayload output
        }
    }

    method notNullBottomUpRoundTrip {
        in {
            NotNullListener.NotNullListener listener
        }
        out {
            Boolean result
        }
    }

    method notNullBottomUpAttributeRoundTrip {
        in {
            NotNullListener.NotNullListener listener
        }
        out {
            Boolean result
        }
    }
}

interface NotNullListener {

    public typedef NotNullListener is undefined

    method notNullBottomUpRoundTrip {
        in {
            NotNullPayload.NotNullPayload input
        }
        out {
            NotNullPayload.NotNullPayload output
        }
    }

    attribute NotNullPayload.NotNullPayload notNullAttribute
}

interface NotNullPayload {

    public typedef NotNullPayload is undefined

    method poke {
        out {
            Boolean result
        }
    }

    method create {
        out {
            NotNullPayload result
        }
    }
}
