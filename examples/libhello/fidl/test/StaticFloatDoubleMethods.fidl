package test

interface StaticFloatDoubleMethods {
    version {
        major 0
        minor 1
    }

    <** @description: Method that takes a float as input and returns it **>
    method returnFloat {
        in { Float inputNumber }
        out { Float outputNumber }
    }

    <** @description: Method that takes a float as input and returns it incremented by 1 **>
    method returnIncrementedFloat {
        in { Float inputNumber }
        out { Float outputNumber }
    }

    <** @description: Method that takes 2 floats and returns the sum **>
    method sumTwoFloats {
        in {
            Float inputNumber1
            Float inputNumber2
        }
        out { Float sum }
    }

    <** @description: Method that takes a double as input and returns it **>
    method returnDouble {
        in { Double inputNumber }
        out { Double outputNumber }
    }

    <** @description: Method that takes a double as input and returns it incremented by 1 **>
    method returnIncrementedDouble {
        in { Double inputNumber }
        out { Double outputNumber }
    }

    <** @description: Method that takes 2 doubles and returns the sum **>
    method sumTwoDoubles {
        in {
            Double inputNumber1
            Double inputNumber2
        }
        out { Double sum }
    }
}
