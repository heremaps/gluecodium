package test

interface NullableStatic {

    public struct NullableStruct {
        NullablePayload.NullablePayload nullableField
    }

    method nullableTopDownRoundTrip {
        in {
            NullablePayload.NullablePayload input
        }
        out {
            NullablePayload.NullablePayload output
        }
    }

    method nullableBottomUpRoundTrip {
        in {
            NullableInstanceListener.NullableInstanceListener listener
        }
        out {
            Boolean result
        }
    }

    method nullableBottomUpNullRoundTrip {
        in {
            NullableInstanceListener.NullableInstanceListener listener
        }
        out {
            Boolean result
        }
    }

    method nullableBottomUpAttributeRoundTrip {
        in {
            NullableInstanceListener.NullableInstanceListener listener
        }
        out {
            Boolean result
        }
    }

    method nullableBottomUpAttributeNullRoundTrip {
        in {
            NullableInstanceListener.NullableInstanceListener listener
        }
        out {
            Boolean result
        }
    }
}

interface NullableInstanceListener {

    public typedef NullableInstanceListener is undefined

    method nullableBottomUpRoundTrip {
        in {
            NullablePayload.NullablePayload input
        }
        out {
            NullablePayload.NullablePayload output
        }
    }

    attribute NullablePayload.NullablePayload nullableAttribute
}

interface NullablePayload {

    public typedef NullablePayload is undefined

    method poke {
        out {
            Boolean result
        }
    }

    method create {
        out {
            NullablePayload result
        }
    }
}
