package test

interface StaticStringMethods {
    version {
        major 0
        minor 1
    }

    <** @description: Method that takes a string as input and returns it as output **>
    method returnInputString {
        in { String inputString }
        out { String outputString }
    }

    <** @description:
    Method that takes 2 strings as input and returns their concatenation
    as output.
    **>
    method concatenateStrings {
        in {
            String inputString1
            String inputString2
        }
        out { String inputConcatenationString }
    }

    <** @description: Method that always returns "hello" **>
    method returnHelloString {
        out { String helloString }
    }

    <** @description: Method that always returns "" **>
    method returnEmpty {
        out { String emptyString }
    }
}
