package test

interface ListenerWithErrors {

    public typedef ListenerWithErrors is undefined

    public enumeration ErrorEnum {
        NONE
        BROKEN
    }

    method getMessage {
        out {
            String message
        }
        error ErrorEnum
    }

    method setMessage {
        in {
            String message
        }
        error ErrorEnum
    }
}

interface ErrorMessenger {
    public typedef ErrorMessenger is undefined

    method getMessage {
        in {
            ListenerWithErrors.ListenerWithErrors listener
        }
        out {
            String message
        }
        error ListenerWithErrors.ErrorEnum
    }

    method setMessage {
        in {
            ListenerWithErrors.ListenerWithErrors listener
            String message
        }
        error ListenerWithErrors.ErrorEnum
    }

    method create {
        out {
            ErrorMessenger.ErrorMessenger newInstance
        }
    }
}
