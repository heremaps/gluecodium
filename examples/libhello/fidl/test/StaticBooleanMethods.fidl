package test

interface StaticBooleanMethods {
    version {
        major 0
        minor 1
    }

    <** @description: Static test method which takes a boolean and returns inverted boolean **>
    method returnInvertedBoolean {
        in {
            Boolean input
        }
        out {
            Boolean output
        }
    }

    <** @description: Static test method which takes two boolean and returns logical AND of the parameters **>
    method returnAndBoolean {
        in {
            Boolean input1
            Boolean input2
        }
        out {
            Boolean output
        }
    }
}
