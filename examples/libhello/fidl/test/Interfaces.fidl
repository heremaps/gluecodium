interface SimpleInterfaceOne {

    public typedef SimpleInterfaceOne is undefined

    method setStringValue {
        in {
            String stringValue
        }
    }

    method getStringValue {
        out {
           String stringValue
        }
    }
}

interface SimpleInterfaceTwo {

    public typedef SimpleInterfaceTwo is undefined

    method setStringValue {
        in {
            String stringValue
        }
    }

    method getStringValue {
        out {
           String stringValue
        }
