}

interface NestedInterfaceOne {

    public typedef NestedInterfaceOne is undefined

    method setSameTypeInterfaces {
        in {
            SimpleInterfaceOne.SimpleInterfaceOne interfaceOne
            SimpleInterfaceOne.SimpleInterfaceOne interfaceTwo
        }
    }

    method getInterfaceOne {
        out {
            SimpleInterfaceOne.SimpleInterfaceOne interfaceOne
        }
    }

    method getInterfaceTwo {
        out {
            SimpleInterfaceOne.SimpleInterfaceOne interfaceTwo
        }
    }
}

interface NestedInterfaceTwo {

    public typedef NestedInterfaceTwo is undefined

    method setMultipleTypeInterfaces {
        in {
            SimpleInterfaceOne.SimpleInterfaceOne interfaceOne
            SimpleInterfaceTwo.SimpleInterfaceTwo interfaceTwo
            NestedInterfaceOne.NestedInterfaceOne nestedInterface
        }
    }

    method getInterfaceOne {
        out {
            SimpleInterfaceOne.SimpleInterfaceOne interface1
        }
    }

    method getInterfaceTwo {
        out {
            SimpleInterfaceTwo.SimpleInterfaceTwo interface2
        }
    }

    method getNestedInterface {
        out {
            NestedInterfaceOne.NestedInterfaceOne interface3
        }
    }

    method setSelfInterface {
        in {
            NestedInterfaceTwo selfInterface
        }
    }

    method getSelfInterface {
        out {
            NestedInterfaceTwo selfInterface
        }
    }
}

