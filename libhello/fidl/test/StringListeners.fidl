package test

interface StringListener {

    public typedef StringListener is undefined

    <**@description: The method to call on the listener when the log message is output **>
    method onMessage {
        in {
            String message
        }
    }
}

interface DummyLogger {

    <**@description: Send the message to our Mobile overlords through the registered listeners **>
    method relayMessage {
        in {
            StringListener.StringListener listener
            String message
        }
    }
}
