package test

<** @description:
    Regression test for struct containing an instance
**>
interface InstanceInStruct {

    public typedef InstanceInStruct is undefined
    public struct SelfHolder {
        InstanceInStruct mySelf
    }

    public struct NotNullSelfHolder {
        InstanceInStruct mySelf
    }

    public struct ExtendedSelfHolder extends InstanceInStruct.SelfHolder {
        String additionalField
    }

    method setStringValue {
        in {
            String stringValue
        }
    }

    method getStringValue {
        out {
           String stringValue
        }
    }

    method create {
        out {
            InstanceInStruct instance
        }
    }

    method createInStruct {
        out {
            SelfHolder wrappedInstance
        }
    }

    method createInInheritedStruct {
        out {
            ExtendedSelfHolder wrappedInstance
        }
    }

    method createNullInStruct {
        out {
            SelfHolder wrappedInstance
        }
    }

    method createInNotNullStruct {
        out {
            NotNullSelfHolder wrappedInstance
        }
    }

    method createInEmptyNotNullStruct {
        out {
            NotNullSelfHolder wrappedInstance
        }
    }
}
