package test

import test.TypeCollection.Point
import test.TypeCollection.Line
import test.TypeCollection.Color
import test.TypeCollection.ColoredLine
import test.TypeCollection.AllTypesStruct

class PlainDataStructuresFromTypeCollection {
    // Static method to create a point
    static fun createPoint(
        x: Double,
        y: Double
    ): Point
    // Static method to exchange x and y values of a point
    static fun swapPointCoordinates(
        input: Point
    ): Point
    // Static test method which creates a colored line
    static fun createLine(
        pointA: Point,
        pointB: Point
    ): Line
    // Static test method which creates a colored line
    static fun createColoredLine(
        line: Line,
        color: Color
    ): ColoredLine
    // Static test method which modifies the AllTypesStruct
    static fun modifyAllTypesStruct(
        input: AllTypesStruct
    ): AllTypesStruct
}
