package test

class StaticStringMethods {
    // Method that takes a string as input and returns it as output
    static fun returnInputString(
        inputString: String
    ): String
    // Method that takes 2 strings as input and returns their concatenation
    // as output.
    static fun concatenateStrings(
        inputString1: String,
    
        inputString2: String
    ): String
    // Method that always returns "hello"
    static fun returnHelloString(): String
    // Method that always returns ""
    static fun returnEmpty(): String
}
