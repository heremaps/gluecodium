package test

@Skip(Java, Swift)
exception ThrowMe(String)

@Skip(Java, Swift)
class AsyncClass {
    constructor create()
    @Async
    fun asyncVoid(input: Boolean)
    @Async
    fun asyncVoidThrows(shouldThrow: Boolean) throws ThrowMe
    @Async
    fun asyncInt(input: Boolean): Int
    @Async
    fun asyncIntThrows(shouldThrow: Boolean): Int throws ThrowMe
    @Async
    static fun asyncStatic(input: Boolean)
}

@Skip(Java, Swift)
struct AsyncStruct {
    stringField: String

    @Async
    fun asyncVoid(input: Boolean)
    @Async
    fun asyncVoidThrows(input: Boolean) throws ThrowMe
    @Async
    fun asyncInt(input: Boolean): Int
    @Async
    fun asyncIntThrows(input: Boolean): Int throws ThrowMe
    @Async
    static fun asyncStatic(input: Boolean)
}
