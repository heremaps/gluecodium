package test

class StaticFloatDoubleMethods {
    // Method that takes a float as input and returns it
    static fun returnFloat(
        inputNumber: Float
    ): Float
    // Method that takes a float as input and returns it incremented by 1
    static fun returnIncrementedFloat(
        inputNumber: Float
    ): Float
    // Method that takes 2 floats and returns the sum
    static fun sumTwoFloats(
        inputNumber1: Float,
    
        inputNumber2: Float
    ): Float
    // Method that takes a double as input and returns it
    static fun returnDouble(
        inputNumber: Double
    ): Double
    // Method that takes a double as input and returns it incremented by 1
    static fun returnIncrementedDouble(
        inputNumber: Double
    ): Double
    // Method that takes 2 doubles and returns the sum
    static fun sumTwoDoubles(
        inputNumber1: Double,
    
        inputNumber2: Double
    ): Double
}
