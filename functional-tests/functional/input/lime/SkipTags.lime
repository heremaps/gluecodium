package test

class SkipTagsOnly {
    @Skip(Lite)
    static fun skipUnquoted()
    @Skip("Lite")
    static property skipQuoted: String
}

types SkipTypesTags {
  @Skip(Lite)
  enum SkipMe {
      NOPE
  }

  @Skip(Lite)
  struct SkipMeToo {
      `field`: SkipMe
  }

  const PlaceHolder: Boolean = true

  @Java(Skip = "Lite")
  @Swift(Skip = "Lite")
  @Dart(Skip = "Lite")
  enum SkipPlatform { N0 }
}

struct SkipField {
  `field`: String
  @Skip(Lite)
  noField: SkipTypesTags.SkipMe
}

class SkippedFunctionClass {
    @Java(Skip = "Lite")
    @Swift(Skip = "Lite")
    @Dart(Skip = "Lite")
    fun doFoo(input: SkipTypesTags.SkipPlatform)
}
