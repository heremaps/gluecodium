package test

class StaticIntMethods {
    // Method that takes a int8 as input and returns next integer to input as output where input
    // must be less than 127
    static fun returnNextNumberInt8(
        inputNumber: Byte
    ): Byte
    // Method that takes 2 Int8 as input and returns their sum as output.
    static fun sumTwoNumbersInt8(
        inputNumber1: Byte,
    
        inputNumber2: Byte
    ): Byte
    // Method that always returns 2
    static fun returnPrimeInt8(): Byte
    // Method that takes a UInt8 as input and returns next integer to input as output where input
    // must be less than 255
    static fun returnNextNumberUint8(
        inputNumber: UByte
    ): UByte
    // Method that takes 2 UInt8 as input and returns their sum as output.
    static fun sumTwoNumbersUint8(
        inputNumber1: UByte,
    
        inputNumber2: UByte
    ): UByte
    // Method that always returns 131
    static fun returnPrimeUint8(): UByte
    // Method that takes a UInt16 as input and returns next integer to input as output where input
    // must be less than 32767
    static fun returnNextNumberInt16(
        inputNumber: Short
    ): Short
    // Method that takes 2 Int16 as input and returns their sum as output.
    static fun sumTwoNumbersInt16(
        inputNumber1: Short,
    
        inputNumber2: Short
    ): Short
    // Method that always returns 257
    static fun returnPrimeInt16(): Short
    // Method that takes a UInt16 as input and returns next integer to input as output where input
    // must be less than 65535
    static fun returnNextNumberUint16(
        inputNumber: UShort
    ): UShort
    // Method that takes 2 UInt16 as input and returns their sum as output.
    static fun sumTwoNumbersUint16(
        inputNumber1: UShort,
    
        inputNumber2: UShort
    ): UShort
    // Method that always returns 32771
    static fun returnPrimeUint16(): UShort
    // Method that takes a Int32 as input and returns next integer to input as output where input
    // must be less than 2147483647
    static fun returnNextNumberInt32(
        inputNumber: Int
    ): Int
    // Method that takes 2 Int32 as input and returns their sum as output.
    static fun sumTwoNumbersInt32(
        inputNumber1: Int,
    
        inputNumber2: Int
    ): Int
    // Method that always returns 65537
    static fun returnPrimeInt32(): Int
    // Method that takes a Uint32 as input and returns next integer to input as output where input
    // must be less than 4294967295
    static fun returnNextNumberUint32(
        inputNumber: UInt
    ): UInt
    // Method that takes 2 Uint32 as input and returns their sum
    // as output.
    static fun sumTwoNumbersUint32(
        inputNumber1: UInt,
    
        inputNumber2: UInt
    ): UInt
    // Method that always returns 2147483659
    static fun returnPrimeUint32(): UInt
    // Method that takes a Int64 as input and returns next integer to input as output where input
    // must be less than 2^63-1
    static fun returnNextNumberInt64(
        inputNumber: Long
    ): Long
    // Method that takes 2 Int64 as input and returns their sum
    // as output.
    static fun sumTwoNumbersInt64(
        inputNumber1: Long,
    
        inputNumber2: Long
    ): Long
    // Method that always returns 4294967311
    static fun returnPrimeInt64(): Long
    // Method that takes a Uint64 as input and returns next integer to input as output where input
    // must be less than 2^64-1
    static fun returnNextNumberUint64(
        inputNumber: ULong
    ): ULong
    // Method that takes 2 Uint64 as input and returns their sum as output.
    static fun sumTwoNumbersUint64(
        inputNumber1: ULong,
    
        inputNumber2: ULong
    ): ULong
    // Method that always returns 4294967311
    static fun returnPrimeUint64(): ULong
}
