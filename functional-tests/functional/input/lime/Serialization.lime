package test

types Serialization {
    @Serializable
    struct SerializableStruct {
        boolField: Boolean
        byteField: Byte
        shortField: Short
        intField: Int
        longField: UInt
        floatField: Float
        doubleField: Double
        stringField: String
        structField: NestedSerializableStruct
        byteBufferField: Blob
        arrayField: List<String>
        structArrayField: NestedStructArray
        mapField: ErrorCodeToMessageMap
        setField: Set<String>
        enumSetField: Set<FooEnum>
        enumField: FooEnum
    }

    enum FooEnum {
        FOO,
        BAR = 7
    }

    @Serializable
    @Equatable
    struct NestedSerializableStruct {
        someField: String
    }

    typealias NestedStructArray = List<NestedSerializableStruct>
    typealias ErrorCodeToMessageMap = Map<Int, String>
}
